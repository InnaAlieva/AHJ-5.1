{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-template-curly-in-string": "off",
    "array-callback-return": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],

    "import/no-extraneous-dependencies": [
      "error",
      { "devDependencies": true}
    ]

  }
}